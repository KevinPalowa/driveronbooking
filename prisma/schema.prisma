generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOWDATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String
}

model Role {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model Route {
  id          Int    @id @default(autoincrement())
  destination String
  estimation  String
}
